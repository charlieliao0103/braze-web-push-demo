{
    "headers": [
        {
            "source": "/sw.js",
            "headers": [
                {
                    "key": "Service-Worker-Allowed",
                    "value": "/"
                },
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                },
                {
                    "key": "Cache-Control",
                    "value": "no-cache, max-age=0"
                }
            ]
        },
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "script-src 'self' https://js.appboycdn.com 'unsafe-inline'; worker-src 'self' blob:; connect-src 'self' https://*.braze.com"
                }
            ]
        }
    ]
}
